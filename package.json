  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd client && yarn run build",
    "install-client": "cd client && yarn install",
    "heroku-postbuild": "yarn run install-client && yarn run build",
    "start": "node server.js",
    "client": "cd client && yarn start",
    "dev": "concurrently -n 'server,client' -c 'red,green'  \"node server.js\" \"yarn run client\""
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.0",
    "concurrently": "^5.0.0",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "mongoose": "^5.7.9",
    "popper.js": "^1.16.1",
    "reactstrap": "^8.5.1"
  }
}
